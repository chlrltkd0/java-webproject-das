<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<settings>
	  <setting name="mapUnderscoreToCamelCase" value="true"/>
	</settings>

	<typeAliases>
		<typeAlias alias="PartyVO" type="com.das.biz.model.party.PartyVO"></typeAlias>
		<typeAlias alias="ContactVO" type="com.das.biz.model.party.contactpoint.ContactVO"></typeAlias>
		<typeAlias alias="ContactTypeVO" type="com.das.biz.model.party.contactpoint.ContactTypeVO"></typeAlias>
		<typeAlias alias="DeliveryVO" type="com.das.biz.model.delivery.DeliveryVO"></typeAlias>
		<typeAlias alias="DeliveryImgVO" type="com.das.biz.model.delivery.DeliveryImgVO"></typeAlias>
		<typeAlias alias="SendingVO" type="com.das.biz.model.delivery.SendingVO"/>
		<typeAlias alias="ReceivingVO" type="com.das.biz.model.delivery.ReceivingVO"/>
		<typeAlias alias="LocationVO" type="com.das.biz.model.location.LocationVO"></typeAlias>
		<typeAlias alias="SearchConditionVO" type="com.das.biz.model.delivery.SearchConditionVO"></typeAlias>
		<typeAlias alias="PartyLocationVO" type="com.das.biz.model.partylocation.PartyLocationVO"></typeAlias>
		<typeAlias alias="DeliverySearchOption" type="com.das.biz.model.delivery.DeliverySearchOption"></typeAlias>
		<typeAlias alias="ChattingVO" type="com.das.biz.model.chatting.ChattingVO"></typeAlias>
		<typeAlias alias="StartEndDateVO" type="com.das.biz.model.chart.StartEndDateVO"></typeAlias>
		<typeAlias alias="DeliveryVolumeVO" type="com.das.biz.model.chart.DeliveryVolumeVO"></typeAlias>
		<typeAlias alias="PushData" type="com.das.biz.model.push.PushData"></typeAlias>
		<typeAlias alias="BoardVO" type="com.das.biz.model.board.BoardVO"></typeAlias>
		<typeAlias alias="BoardCategoryVO" type="com.das.biz.model.board.BoardCategoryVO"></typeAlias>
		<typeAlias alias="BoardListVO" type="com.das.biz.model.board.BoardListVO"></typeAlias>
		<typeAlias alias="Moving" type="com.das.biz.model.action.Moving"></typeAlias>
		<typeAlias alias="Staying" type="com.das.biz.model.action.Staying"></typeAlias>
		<typeAlias alias="Pattern" type="com.das.biz.model.pattern.Pattern"></typeAlias>
	</typeAliases>
	
	<mappers>
		<mapper resource="mappings/board-mapping.xml"/>
		<mapper resource="mappings/boardcategory-mapping.xml"/>
		<mapper resource="mappings/party-mapping.xml"/>
		<mapper resource="mappings/contact-mapping.xml"/>
		<mapper resource="mappings/delivery-mapping.xml"/>
		<mapper resource="mappings/location-mapping.xml"/>
		<mapper resource="mappings/partylocation-mapping.xml"/>
		<mapper resource="mappings/pattern-mapping.xml"/>
		<mapper resource="mappings/chatting-mapping.xml"/>
		<mapper resource="mappings/chart-mapping.xml"/>
		<mapper resource="mappings/pushdata-mapping.xml"/>
		<mapper resource="mappings/action-mapping.xml"/>
	</mappers>
</configuration>
